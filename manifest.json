{
     "name": "TSDBConsole",
     "description": "Management Console for OpenTSDB",
     "version": "1.0",
     "app": {
          "background": {
               "scripts": [
                   "/js/jquery-2.0.3.js",
                    "background.js"
     	    ]
	    }
     },
     "icons": {
          "128": "/img/tsdbicon_128.png",
          "16": "/img/tsdbicon_16.png"
     },
     "permissions": [
          "alarms",
          "clipboardRead",
          "clipboardWrite",
          "notifications",
          "syncFileSystem",
          "unlimitedStorage",
          "storage",
          "tts",
          "contextMenus",
          "fileBrowserHandler",
          "idle",
          "http://*/*",
          {"socket" : ["tcp-connect", "tcp-listen", "udp-send-to", "udp-bind"]}
          
     ],
     "manifest_version": 2
}